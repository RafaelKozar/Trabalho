<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">


    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap core CSS -->
    <link href="http://getbootstrap.com/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="http://getbootstrap.com/dist/js/bootstrap.min.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="http://getbootstrap.com/assets/js/ie10-viewport-bug-workaround.js"></script>
    <link rel="stylesheet" href="../stylesheets/estilo.css" />

</head>
<body>
    <% include header.ejs %>      

    <div class="col-sm-1 col-md-1 col-lg-1 col-xs-1 "></div>
    <div class="col-sm-12 col-md-10 col-lg-10  col-xs-12  conteudo">
        <form class="form-signin">
                
            <div class="row">
                <div class="col-md-12">
                    <div class="panel-group" id="accordion">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h4 class="panel-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                                        <span class="glyphicon glyphicon-file page-header"></span>Atendimento
                                    </a>
                                </h4>
                            </div>

                            <div id="collapseOne" class="panel-collapse collapse in">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <% if (typeof atendimento != 'undefined') { %>

                                                    <div class="form-group col-lg-6 col-md-6">
                                                        <label>Paciente</label>
                                                        <input type="text" id="paciente" name="paciente" value="<%= atendimento.paciente%>" class="campo form-control"  readonly/>
                                                    </div>

                                            <div class="form-group col-lg-6 col-md-6 ">
                                                <label>Usuário</label>
                                                <input type="text" id="user" name="user" value="<%= atendimento.user %>" class="form-control" readonly/>
                                            </div>

                                            <div class="form-group col-lg-6 col-md-6 ">
                                                <label>Horário do Atendimento</label>
                                                <input type="text" id="horarioDoAtendimento" name="horarioDoAtendimento" value="<%= atendimento.horarioDoAtendimento %>" class="form-control" readonly />
                                            </div>

                                            <div class="form-group col-lg-6 col-md-6 ">
                                                <label>Data</label>
                                                <input type="text" id="data" name="data" value="<%= atendimento.dataFormatada %>" class="form-control" readonly />
                                            </div>

                                            <div class="col-lg-3 col-md-3"></div>
                                            <div class="form-group col-lg-6 col-md-6">
                                                <label>Tipo do Atendimento</label>
                                                <input type="text" id="tipoAtendimento" name="tipoAtendimento" value="<%= atendimento.tipoAtendimento%>"  class="form-control" readonly />
                                            </div>
                                            <div class="col-lg-3 col-md-3"></div>

                                            <div class="form-group col-lg-12 col-md-12">
                                                <label>Atendimento</label>
                                                <textarea id="atendimentoTexto" name="atendimentoTexto" class="form-control" readonly ><%= atendimento.atendimentoTexto %></textarea>
                                            </div>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>


</body>



</html>
